    "version": "1.0.1",
    "author": {
        "name": "Floris Steenkamp"
    },
    "license": "MIT",
    "main": "node/index",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FlorisSteenkamp/FloBezier3.git"
    },
    "bugs": {
        "url": "https://github.com/FlorisSteenkamp/FloBezier3/issues"
    },
    "keywords": [
        "bezier",
        "beziers",
        "quadratic",
        "cubic",
        "intersection"
    ],
    "dependencies": {
        "flo-draw": "^1.0.0",
        "flo-gauss-quadrature": "^1.0.1",
        "flo-graham-scan": "^1.0.1",
        "flo-memoize": "^0.1.2",
        "flo-numerical": "^1.0.1",
        "flo-poly": "^4.0.0",
        "flo-vector2d": "^2.0.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^7.0.2",
        "chai": "^4.2.0",
        "mocha": "^7.1.1",
        "nyc": "^15.0.1",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.17",
        "ts-loader": "^7.0.1",
        "ts-node": "^8.8.2",
        "typedoc": "^0.17.4",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    },
    "scripts": {
        "build": "npm run build-browser && npm run build-node",
        "build-node": "rimraf node && tsc",
        "build-browser": "rimraf browser && webpack",
        "test": "nyc mocha",
        "a": "mocha --require ts-node/register --require source-map-support/register ./test/implicit-form.spec.ts"
