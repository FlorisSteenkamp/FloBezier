{
    "name": "flo-bezier3",
    "description": "Purely functional linear, quadratic and cubic bezier library, including accurate, robust bezier intersection.",
    "version": "3.0.0",
    "author": {
        "name": "Floris Steenkamp"
    },
    "license": "MIT",
    "type": "module",
    "main": "./node/index.js",
    "exports": "./node/index.js",
    "engines": {
        "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FlorisSteenkamp/FloBezier3.git"
    },
    "bugs": {
        "url": "https://github.com/FlorisSteenkamp/FloBezier3/issues"
    },
    "keywords": [
        "bezier",
        "beziers",
        "quadratic",
        "cubic",
        "intersection"
    ],
    "dependencies": {
        "big-float-ts": "^3.0.1",
        "double-double": "^2.0.5",
        "flo-gauss-quadrature": "^2.1.0",
        "flo-graham-scan": "^2.0.0",
        "flo-ll-rb-tree": "^0.3.1",
        "flo-poly": "^6.0.3",
        "flo-vector2d": "^4.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^17.0.5",
        "c8": "^7.10.0",
        "chai": "^4.3.4",
        "chai-asserttype": "^1.0.5",
        "circular-dependency-plugin": "^5.2.2",
        "mocha": "^9.1.3",
        "paper": "^0.12.15",
        "resolve-typescript-plugin": "^1.1.1",
        "rimraf": "^3.0.2",
        "squares-rng": "^2.0.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1"
    },
    "scripts": {
        "build": "npm run build-browser && npm run build-node",
        "build-node": "rimraf node && tsc",
        "build-browser": "webpack",
        "test": "mocha",
        "coverage": "c8 mocha",
        "performance": "cd performance & ts-node bezier-bezier-intersection.ts",
        "a": "mocha ./test/intersection/bezier-bezier-intersection/intersection-performance.spec.ts --timeout 6000 --require ts-node/register --require source-map-support/register",
        "b": "mocha ./test/implicit-form/implicit-form.spec.ts --timeout 1000 --require ts-node/register --require source-map-support/register",
        "c": "mocha ./test/intersection/bezier-bezier-intersection/coeffs-3.spec.ts --timeout 6000 --require ts-node/register --require source-map-support/register",
        "d": "mocha ./test/intersection/bezier-bezier-intersection/bezier-bezier-intersection.spec.ts --timeout 6000 --require ts-node/register --require source-map-support/register"
    }
}
