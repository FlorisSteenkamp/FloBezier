    "mocha": "^4.1.0",
    "nyc": "^11.9.0",
    "ts-loader": "^5.2.2",
    "tsify": "^3.0.4",
    "typescript": "^2.9.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "rimraf": "latest"
