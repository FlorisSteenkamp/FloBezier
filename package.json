{
    "name": "flo-bezier3",
    "description": "Purely functional linear, quadratic and cubic bezier library, including accurate, robust bezier intersection.",
    "version": "3.0.0",
    "author": {
        "name": "Floris Steenkamp"
    },
    "license": "MIT",
    "main": "node/index",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FlorisSteenkamp/FloBezier3.git"
    },
    "bugs": {
        "url": "https://github.com/FlorisSteenkamp/FloBezier3/issues"
    },
    "keywords": [
        "bezier",
        "beziers",
        "quadratic",
        "cubic",
        "intersection"
    ],
    "dependencies": {
        "big-float-ts": "^2.0.0",
        "double-double": "^1.0.4",
        "flo-gauss-quadrature": "^1.0.1",
        "flo-graham-scan": "^1.0.2",
        "flo-poly": "^5.0.2",
        "flo-vector2d": "^3.0.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.16",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "chai": "^4.3.4",
        "circular-dependency-plugin": "^5.2.2",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0",
        "paper": "^0.12.15",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "ts-loader": "^8.1.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4",
        "webpack": "^5.31.2",
        "webpack-cli": "^4.6.0"
    },
    "scripts": {
        "build": "npm run build-browser && npm run build-node",
        "build-node": "rimraf node && tsc",
        "build-browser": "webpack",
        "test": "nyc mocha",
        "performance": "cd performance & ts-node bezier-bezier-intersection.ts",
